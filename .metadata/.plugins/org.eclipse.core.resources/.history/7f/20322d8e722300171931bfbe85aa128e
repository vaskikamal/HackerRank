package vaski.warmup;

import java.util.Scanner;

public class StairCase {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		for (int i = 1; i <= n; i++) {
			int spaceCnt = n - i;
			StringBuilder bldr = new StringBuilder();
			System.out.println();
		}
	}
}
