package vaski.warmup;

import java.util.Arrays;
import java.util.Scanner;

public class MinMax {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		int N = 5;

		long[] arr = new long[N];
		for (int i = 0; i < N; i++) {
			arr[i] = in.nextLong();
		}
		Arrays.sort(arr);

		int min = 0, max = 0;
		for (int i = 1; i < N; i++) {
			max += arr[i];
		}
		for (int i = N - 1; i > 0; i++) {
			min += arr[i];
		}
		
		System.out.println( max +" " + min);
	}
}
